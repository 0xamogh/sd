{
  "name": "@groceristar/sd-wrapper",
  "version": "1.0.16",
  "license": "MIT",
  "scripts": {
    "bundle": "rollup -c",
    "dev": "rollup -c -w",
    "build": "npm run build:js",
    "build:js": "babel src -d dist --copy-files",
    "build:test": "yarn clean && babel src -d dist --copy-files",
    "#prepare": "yarn clean",
    "clean": "shx rm -rf ./coverage && shx rm -rf ./dist",
    "zprepublish": "yarn clean && yarn code-fix && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "files": "node --experimental-modules dist/index.js",
    "files2": "node --experimental-modules files.js",
    "jsonlint": "node jsonlint.config.js",
    "lint": "standard",
    "code-fix": "standard --fix",
    "format": "prettier"
  },
  "description": "module to use static JSON files with groceristar projects",
  "main": "dist/index.cjs",
  "module": "dist/index.es.js",
  "browser": "dist/index.iife.js",
  "repository": "https://github.com/GroceriStar/sd",
  "author": "Arthur Tkachenko",
  "private": false,
  "release": {
    "publishDir": "dist"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "coveralls": "^3.0.2",
    "jest": "^24.1.0",
    "jest-json-schema": "^2.0.1",
    "jsonlint": "^1.6.3",
    "nodemon": "^1.17.5",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-copy-assets": "^1.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "shx": "^0.3.2",
    "standard": "^12.0.1"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "uuid": "^3.3.2"
  },
  "files": [
    "dist"
  ]
}
